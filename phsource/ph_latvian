phoneme i
  vowel starttype #i endtype #i
  length 100
  FMT(vowel/i_6)
endphoneme

phoneme i2
  vowel starttype #i endtype #i
  length 100
  IfNextVowelAppend(;)
  FMT(vowel/i_2)
endphoneme


phoneme i3
  vowel starttype #i endtype #i
  length 100
  IfNextVowelAppend(;)
  FMT(vowel/i_3)
endphoneme


phoneme a
  vowel starttype #a endtype #a
  length 100
  FMT(vowel/aa_7) // possible variants: a_3, a_5 or aa_7
endphoneme

phoneme a:
  vowel starttype #a endtype #a
  length 300
  long
  FMT(vowel/aa_9) // was a_3 or aa_9
endphoneme


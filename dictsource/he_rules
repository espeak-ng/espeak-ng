// rules for Hebrew written by English characters
// Sometimes, Hebrew involve English characters


.group a
	a	_^_en
	?1 a	a
	// ?1 a ('	A


.group aa
	aa	_^_en
	//?1 aa	A


.group b
	b	_^_en
    ?1 b	b


.group c
	c	_^_en
	?1 c (e	s
	?1 c (y	s
	?1 c (i	s
	?1 c	k



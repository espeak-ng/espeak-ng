	?1 t	t


.group th
	th	_^_en



.group u
	u	_^_en
	?1 u	u


.group v
	v	_^_en
	?1 v	v


.group w
	w	_^_en
	?1 w	w


.group x
	x	_^_en
	?1 x	ks


.group y
	y	_^_en
	?1 y	j


.group z
	z	_^_en
	?1 z	z


// Others mathematics characters
.group
	 . (D     	noq1te:
	±	plus||minus
	>=	gad@ol||o@||Save
	<=	kat@an||o@||Save

// translation rules for Latvian
// This file is UTF-8 encoded

// +---------------------------+
// |     Suffixes of nouns     |
// +---------------------------+

.L01    s š a am u ā i iem us os   // suffixes for masculine words finishing with -s or -š (1. declination)
.L02    is a im i ī u iem us os    // suffixes for masculine words finishing with -is (2. declination)
.L03    us um u ū i iem os         // suffixes for masculine words finishing with -us (3. declination)
.L04    a as ai u ā u ām ās        // suffixes for feminine words finishing with -a  (4. declination)
.L05    e es ei i ē u ēm ēs        // suffixes for feminine words finishing with -e  (5. declination)
.L06    s ij i ī is u īm īs        // suffixes for feminine words finishing with -s  (6. declination) 

.L07    iem os i u                 // suffixes for plural masculine words finishing with i

// +---------------------------+
// |    Suffixes of verbs      |
// +---------------------------+

.L11    t ties                     // suffixes of verbs in infinitive
.L12    amies amas amās ama ami  ams

// +---------------------------+
// |      Other suffixes       |
// +---------------------------+

.L13 li ni ti                      // suffixes of adverbs


// +---------------------------+
// | Pronunciation of e and ē  |
// +---------------------------+

// E narrowers
.L21    i ī ie ei                  // vowel narrowers of e
.L22    j k ģ ļ ņ ŗ š ž č dž       // consonant narrowers of e
.L26    f h ḩ                      // international consonants as a narrowers of e 
.L23    s i m                      // remaining ending of the 2. and 5. decl. words

// E wideners
.L24    a ā u ū ai au o            // vowel wideners of e
.L25    r                          // consonant wideners of e


// +---------------------------+
// |     Pronunciation of o    |
// +---------------------------+

// Default rules for o with succeeding consonants
.L40     č ģ ķ ļ ņ ŗ š ž           // uo indicator letters o is spelled as uo with Latvian soft consonants
.L41    f h w x y                 // o indicators — o is spelled as o with Latin letters

.L42  i e o  // o vowel pre/postfixes
.L43  lg ng zn // o postfixes

.L44  gļ šņ ļš jā // uo postfixes

// +---------------------------+
// |       Vowel groups        |
// +---------------------------+

.L50    a ā
.L51    e ē

// +---------------------------+
// |    Consonant groups       |
// +---------------------------+
.L60    s š
.L61    t d j                    

// +---------------------------+
// |        All letters        |
// +---------------------------+
// These are used for "." like padding
.L88    a ā b c č d e ē f g ģ h ḩ i ī j k ķ l ļ m n ņ o ō p r ŗ s š t u ū v w x y z ž - 

// +---------------------------+
// |   Pronunciation groups    |
// +---------------------------+

.group a
           a               a
           a (a            a_!
           ai              ai
           au              au
           ap              ap        // prefix
   
.group ā
           ā               a:
           ā (ā            a:_!
   
.group b
           b               b
   
.group c
           c               ts
           ch              tS        // Appears mostly in English, not in old latvian writing where should be x
       er) ch (ercogs      tsh
   fridri) ch (s           tsh
   
   
.group č
           č               tS
   
.group d
           d               d
   
.group e
   
           e               E         // Default as wide e
           ei              ei
   
    // narrow e

        a) e (ro           e         // internationalsms
        h) e (+            e         
           e (h+           e

        @) e (_            e
        @) e (L23_         e         // -es, -ei, -em of 2. and 5. decl. words
        @) e (L11_         e         // -et, eties
        @) e (CL11_        e
        @) e (rL02_        e
        @) e (rL05_        e
        @) e (CL06_        e
        @) e (CCL06_       e
        @) e (rA_          e
        @) e (CrA_         e
        @) e (C_           e
        @) e (Cs_          e
 

           e (o            e
           e (CL21         e         // narrowing vowels follow
           e (CCL21        e
           e (CCCL21       e
           e (CCCCL21      e
           e (L22          e       
           e (L26          e 


                 
       _b) e (z            e
       _d) e (poz          e
       _n) e               e
      _pr) e (t            e
       _z) e (m            e

           e (lgL04_       e         // Elga
           e (lzL04_       e         // Elza
       _C) e (_            e         
       _b) e (t_           e         
       _b) e (z_           e
      _dr) e (nL04_        e
       _j) e (b_           e
       _j) e (l_           e
       _m) e (dūzL04_      e
       _m) e (kL04_        e         // Meka
       _s) evastopo (lL05_ evastopo: // Sevastopole
       _t) e (klL04_       e         // Tekla
       _z) e (lmL04_       e         // Zelma

          
    // wide e

           e (L01_         E
           e (CL01_        E
           e (CCL01_       E
           e (CL24         E
           e (CCL24        E
           e (CCCL24       E
           e (L25          E
 
   
.group ē
   
           ē               E:       // Default as wide ē

    // narrow e   
   
        @) ē (_            e:
        @) ē (L23_         e:
        @) ē (L11_         e:
        @) ē (CL11_        e:
        @) ē (rL02_        e:
        @) ē (rL05_        e:
        @) ē (CL06_        e:
        @) ē (CCL06_       e:
        @) ē (rA_          e:
        @) ē (CrA_         e:
        @) ē (C_           e:
        @) ē (Cs_          e:

      _sv) ē (t            e:

       _d) ē (ļ_           e:
       _p) ē (c_           e:
       _s) ē (nL04_        e:        // Sēna
//       _v) ē (l_           e:      // is ambigous "(viņš) vēl" and "vēl nebūs"
   
    // wide e

           ē (L01_         E: 
           ē (CL01_        E:
           ē (CCL01_       E:
           ē (CL24         E:
           ē (CCL24        E:
           ē (CCCL24       E:
           ē (L13_         E:        // cēli, lēni, necerēti

.group f
           f               f
   
.group g
           g               g
        _) g (_            ga
     DD__) g (_            gada
   
   
.group ģ
           ģ               J
   
.group h
           h               h
   
   
.group i
           i               i
           i (i            i_!
           i (ī            i_!
           ie              ie
           iu              iu
   
.group ī
           ī               i:
   
.group j
           j               j
        _) jā              ja:       // prefix
        _) jāno            ja:nuo    // prefix
   
.group k
           k               k
   
.group ķ
           ķ               c
   
.group l
           l               l
           ll              lll       // espeak says doubled ll shorter than doubled ļļ
                                     // no more necessary when phoneme was changed
   
.group ļ
           ļ               l^
        k) ļ (u            lj        // [kl^u] sounds bad in eSpeak
        k) ļ (u            lj
   
.group m
           m               m
           mm              mm:
        _) maz             maz
   
.group n
           n               n
           n (g            N         // now sounds acceptable
           n (k            N
           nn              nn:       // espeak says doubled nn shorter that doubled ņņ
   
.group ņ
           ņ               n^
   
.group o
   // default policy rules
           o ($w_alt       o         // alt words ($alt list in lv_list) are spelled as o
           o (_$w_alt      o:        // o at the end of alt words is spelled as ō
        o) o               o:        // in ..oo.. second o is spelled as ō
   
      L42) o               o:        // o prefixes
           o (L43          o         // o postfixes
      L41) o (L88          o         // o indicators before
   L41L88) o (L88          o
   L41L88L88) o (L88       o
   L41L88L88L88) o (L88    o
           o (L41          o         // o indicators after
           o (L88L41       o
           o (L88L88L41    o
           o (L88L88L88L41 o
   
           o               uo        // words with Latvian roots are more common
           o (L40          uo        // usually uo before soft (Latvian) consonants
      L40) o               uo        // usually uo after soft (Latvian) consonants
   
           o (L44          uo        // uo postfixes
   
     _kil) o               o         // international prefixes
    _mikr) o (+            o         // otherwise conflicts with ..kroko(t/d)..
    _micr) o               o
     _nan) o               o
     _pik) o               o
    _hekt) o               o
       _m) ono             ono:
   _stere) o               o:
   

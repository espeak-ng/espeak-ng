?2	todavia		// yet
?2	contudo		// nevertheless

?2	ou	$u	// or
?2	quer		// whether

?2	assim		// thus
?2	portanto	// therefore

	porque		p'or#ky		// because
?2	porque		pUr#k'e		// because
?2 	(por que) 	por#||k'e	// why
?2 	(por que é) 	por#||k,y||'E	// why
?2	pois		// because
?2	como	$u+	// as
?2	como	$atstart	// as

?2	embora		// although
?2	apesar		// despite


?2	caso			// if

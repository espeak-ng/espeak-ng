.group ә
   ә       w

.group
   гу      gw     // pre-1996
   ҕ       Q"     // pre-1996
   ҕь      Q^     // pre-1996
   ҕу      Q"w     // pre-1996
   ку      k`w     // pre-1996
   ӄу      kw     // pre-1996
   ҟу      q`w     // pre-1996
   ҧ       ph     // pre-1996

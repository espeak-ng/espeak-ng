.group u
        u          u

// General rule: Normal vs. short (glottal) u
// short u followed by double consonant
        u (C%+     ?u        // blusse
   L03) u (ff+     ?O        // bluffe
    sk) u (L02%    ?O        // skuffe, skubbe
     f) u (L06%    ?O        // fuppede
     r) u (L06%    ?O        // siruppen
     C) u (kk+     ?O        // sukker, lukker
     C) u (gg      ?O        // hugge
     b) ug (gjord  u:
     r) u (mmy     ?V        // card game
        u (ds      ?u        // pludselig
     h) utton      ?Vt@n     // name
     b) unny       ?Vni      // name

// General rule: l/d + u + f: O
   L03) u (f       ?O        // luft, duft

        u (se      u:        // muse, huse
        u (set     ?u        // huset - "hus" made short by ph_danish

        ur (gt     o3-       // spurgt
        ure (_     u3-V      // ure, mure
        urer (_    u3-V      // ure, murer

// general rule: [O] after consonant followed by k/g
     C) u (L01t    ?O        // frugt, fugt, lugt, lugte
     j) u (L01L04  u         // Jugoslavien
    gl) u (kose    y         //
     C) u (L01L11  u         // halleluja
    tr) u (ktL04   u         // struktur
     C) u (L01e+   O         // lunge
     t) ucker      VkV       // name
     h) u (je+     u         //
     p) u (ck+     u         //
    br) u (g       u         //
     r) u (g       u         // rug, rugbrød
     l) ugaf       lOg'&     //
     k) u (k       u         //
     b) u (gse     u         // bugsere
    gl) u (g       u         // glugger, glughul
  røgl) u (gt      ?O        //
    fn) u (g       u         // snefnug
        u (gl+     u         // fuglene, uglerne
    br) u (gt      ?O        //
    sl) ug         u         // slugt, sluge - silent g
     s) u (g       u         // sugerør, suggestiv
     l) u (k       ?O        // sluk, pluk, lukke, fluks
     l) u (krL11   u         // lukrativ
        u (ks      ?0        // juks, buks
     l) ukaf       Og'&      // sygelukaf
     p) urple      W:p-@-l   // Purple Heart
     d) u (dley    V         // name
        ulster     VlstV     // name

// general rule: [0] after l/d followed by ns
   L03) u (ns      ?0        //	luns, dunst

// general rule: followed by ng/nk: [O]
        u (ng++    O         // lunge, unge, hungre
        u (nk      O         // sunket, prunk
     j) u (nktur++ O         //
    sp) u (nde     O         // spundet
     j) u (nk      V         // junkaktie
   hal) u (nk      'O        // stressed
    _f) u (nk      V         // funkmusik
    _p) u (nk      V         // punker
   L06) u (nkt     O         // punkt

// general rule: [O] followed by nd
        u (nd+     ?O        // grund, rund,  underligt, undtage
     b) u (nt      ?O        // buntmager
     p) u (nd      ?u        // pund
     t) u (ndå     ?u        // tundåse
   rab) und (us    '?Ond     // stressed

// general rule: Consonant + r + u + p/b = [O]
    Cr) u (L06     O         // broad general rule - skrub
     r) u (L06     O         // sirup, rup
     r) u (f       O         // rufferi

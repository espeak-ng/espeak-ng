        u (dgrundelig+ u     //
        u (imodståelig u     //
        u (surp    u         // usurpator
        u (ni++    u         // uniform, universitet
     t) u (ning+   ju        //
     t) u (ne      ju        //
  prod) u (ceren   ju        //
   _st) u (art     ju        // name
     g) u (den+    ?u        // u is shorter in "guden" than in "gud" (glottal stop)
        u (ge++    u         // ugerne, sluge
        uger (+    u:V       // sluger, slugere
     s) uge (rør   u3        // silent g
     _) u (r++     u         // ur, urrem, uret
     _) u (retfæ++P1 u:      // uretfærdig - prefix

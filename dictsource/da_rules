    _l) ig (_pænt  ek        //
    _l) ig (_ordentligt ek   //
    _l) ig (_godt  ek        //
    _l) ig (_roligt ek       //
     l) ight       aIt       //
     m) ighty      aIti      //
    fr) igea (r    igi       // frigear
        ig (hed+   i         // vittighed, evighed
    _k) ig (_      ig        // kig

// This file is UTF-8 encoded
// translation rules for Gagauz
// Author: Andy Vladescu, andyvladescu770@gmail.com, 2025

// Vowels (latin version)
.L01 ä e i ö ü  // front vowels
.L02 a ı o u    // back vowels


// Vowels (cyrillic version)
.L11 ӓ е и ӧ ӱ  // front vowels
.L12 а о у ы    // back vowels


// Latin group letters

.group a
   a       A

.group ä
   ä       &

.group b
   b       b

.group c
   c       dZ;

.group ç
   ç       tS;
   ça (_   =tS;A

.group d
   d       d

.group e
 _) e      je  // at the beginning of words
    e      ;e

.group ê
   ê       @

.group f
   f       f

.group g
   g       g
   g (L01  J

.group h
 _) h      h  // at the beginning of words
    h      x

.group ı
   ı       I

.group i
   i       i

.group j
   j       Z

.group k
   k       k
   k (L01  c

.group l
   l       l
   l (L02  L
 L02) l    L
   l (L01  l^

.group m
   m       m

.group n
   n       n
   n (L01  n^

.group o
   o       o

.group ö
   ö       W

.group p
   p       p

.group r
   r       r

.group s
   s       s
   s (L01  s;

.group ş
   ş       S

.group t
   t       t

.group ţ
   ţ       ts

.group u
   u       u

.group ü
   ü       y

.group v
   v       v
   v (L02  w

.group y
   y       j

.group z
   z       z
   z (L01  z;
    


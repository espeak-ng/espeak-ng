// This file is UTF8 encoded
// Spelling-to-phoneme words for Belarusian

// Letter names
а       a $u
б       b $u
в       vE $u
г       QE $u
д       dE $u
е       jE $u
ё       jO $u
ж       z. $u
з       z $u
і       i $u
й       i $u
к       ka $u
л       El  $u
м       Em $u
н       En $u
о       O   $u
п       pE  $u
р       Er  $u
с       Es  $u
т       tE  $u
у       u $u
ў       w $u
ф       Ef $u
х       xa $u
ц       tsE $u
ч       ts.E $u
ш       s.a $u
ы       i" $u
ь       m;'ak;:i||znak
э       E $u
ю       ju $u
я       ja $u

// Numbers
_0     n'ul;
_1     adz;'in

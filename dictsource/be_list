_9X    dz;Ev;an'Osta||
_0C    st'O
_2C    dz;v;'Es;ts;E
_3C    tr'i"sta
_4C    ts.at'i"ri"sta
_5C    p;ats;s'Ot
_6C    s.Es;ts;s'Ot

// French translation rules

// This file is UTF-8 encoded

// 2006-11-18  Gilles Casse  <gcasse@oralux.org>
//
// Updated: 2008-06-04 Michel Such  <michel.such@free.fr>
//
//	* The rules are based on Cicero TTS.
//         Y

// Letter classes:
// A  all vowels:  a e i o u y â ä é ê è î ï ô ö ù û
// C  all consonants
// Y  front vowels: e i y é ê è î
// K  not a vowel  (i.e. consonant, space, )


.group a
		ae (_       e		// reggae vitae
		aen         A~		// caen (diff Cicero: aA~)
		ae          ae		// maestro
		aî          E		// maître
		aim (K      W~		// faim
		ain (K      W~		// pain
		ai (_      e		// dirai ferai...
		ais (_      Ez2		// dirais ferais...
	f)	ai (sA      @		// faisons faisan
		a (ill      a
		a (ils_     a
		a (il_      a
		aie (me     E		// paiement
		ai          E		// aile
		am (b       A~		// jambon
		am (p       A~		// camp
		am (m       a		// programmation
		am (m       a		// programmation
		am (n       am		// amnistie
		a  (nnA     a		// manne
		an (C       A~		// ancien
		an (_       A~		// élan
	_C)	aon  (_        A~		// faon, paon, taon
	s)	aoul  (_        u		// saoul
		aoû         ut		// août
		au (l_      O		// Paul
		au          o		// autruche
		aud (_         o		// chaud
		au          o		// autruche
	p)	ay (s       Ei		// pays paysage
		ay (C       E		// aymé
		ay          Ej		// paye
		a           a		// bateau

